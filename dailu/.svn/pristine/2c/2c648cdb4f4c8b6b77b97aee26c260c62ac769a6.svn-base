}
.main .dayWap .rightList .list .infoBox .massage .ms-img img{
	/*width: 100%;*/
	/*height: auto;*/
}
.main .dayWap .rightList .list .infoBox .massage .ms-img p {
	margin-top: 10px;
	
}

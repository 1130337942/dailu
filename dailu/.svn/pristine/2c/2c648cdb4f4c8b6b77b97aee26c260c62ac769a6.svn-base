.main .lineBox .leftMsg .shortCart .short {
	display: inline-block;
	padding-top: 32px;
	position: relative;
	margin-left: 20px;
	color: #999;
	cursor: pointer;
	font-size: 14px;
}
.main .lineBox .leftMsg .shortCart .short::after {
	position: absolute;
	display: block;
	content: '';
	width: 24px;
	height: 24px;
	background: url(/static/v1/img/like.png) 0 0 no-repeat;
	top: 10px;
	margin-left: -10px;
	left: 50%;
}
.leftMsg .shortCart .shareBox{
	padding: 16px;
	width: 355px;
	height: 140px;
	background-color: #fff;
	border-radius: 4px;
	position: absolute;
	top: -170px;
	right: -178px;
	display: none;
	box-shadow: 0px 2px 10px 0px 
		rgba(0, 0, 0, 0.08);
} 

.leftMsg .shortCart .shareBox .copy {
	color: #fff;
	background-color: #659ff5;
	padding: 3px 10px;
	border-radius: 4px;
	cursor: pointer;
	float: right;
}
.leftMsg .shortCart .shareBox .copy:hover{
	background-color: #74adff;
}

.share_code_box {
	/*margin-top:20px;*/
	position: relative;
	height: 100px;
}
.leftMsg .shortCart .shareBox .share_code_img{
	width: 100px;
	height: 100px;
}
.leftMsg .shortCart .shareBox .tip {
	position: absolute;
	display: inline-block;
	top: 44%;
	left: 110px;
	color: #666;
}

.leftMsg .shortCart .shareBox #foo{
	border:none;
	width: 500px;
	font-size: 14px;
	color: #999;
	height: 18px;
	line-height: 18px;
	padding-left: 10px;
	border:1px solid #eee;
	height: 24px;
	width: 246px;
	margin-left: 4px;
}

.leftMsg .shortCart .shareBox  #shareTitle{
	position: absolute;
	border:none;
	padding-left: 10px;
	width: 180px;
	height: 24px;
	border: 1px solid #eee;
	display: inline-block;
	top: 10%;
	left: 100px;
	color: #666;
}
.leftMsg .shortCart .shareBox .titledit,
.leftMsg .shortCart .shareBox .titlesave{
	position: absolute;
	border:none;
	/*padding-left: 10px;*/
	top: 10%;
	right: 0px;
}
.leftMsg .shortCart .shareBox .titledit{
	width: 20px;
	height: 28px;
	background: #fff url(/static/v1/img/tripinfo/edit.png) center center no-repeat;
}
.leftMsg .shortCart .shareBox .titlesave{
	background-color: #659ff5;
	padding: 3px 10px;
	border-radius: 4px;
	cursor: pointer;
	color: #fff;
}
.leftMsg .shortCart .share:hover .shareBox{
	display: block;
}
.main .lineBox .leftMsg .shortCart .see::after{
	background: url(/static/v1/img/tripinfo/see.png) center 1px no-repeat;
}
.main .lineBox .leftMsg .shortCart .comment::after{
	background: url(/static/v1/img/pinlun.png) center center no-repeat;
}
.main .lineBox .leftMsg .shortCart .share::after{
	background: url(/static/v1/img/fengxiang.png) center center no-repeat;
}
.main .lineBox .leftMsg .shortCart .like::after{
	background: url(/static/v1/img/like.png) center center no-repeat;
}
.main .lineBox .leftMsg .shortCart .is_like::after{
	background: url(/static/v1/img/like2.png) center center no-repeat;
}
.main .lineBox .leftMsg .shortCart .collect::after{
	background: url(/static/v1/img/shouchang.png) center center no-repeat;
}
.main .lineBox .leftMsg .shortCart .c_collect::after{
	background: url(/static/v1/img/shouchang2.png) center center no-repeat;
}
.main .dayWap  {
	margin-top: 10px;
}
.main .dayWap .leftLift{
	width: 112px;
	max-height: 100%;
	overflow-y:auto; 
	text-align: center;
	background-color: #ffffff;
	border-radius: 16px;
	border: solid 1px #f0f0f0;
}
.main .dayWap .leftLift .tit {
	padding: 40px 0;
	color: #659ff5;
	width: 72px;
	margin: 0 auto;
	border-bottom: 1px solid #659ff5;
}
.main .dayWap .leftLift .on{
	color: #659ff5;
	position: relative;
}
.main .dayWap .leftLift ul{
	padding: 18px 0;
}

.main .dayWap .leftLift li{
	line-height: 40px;
}
.main .dayWap .leftLift li a:hover {
	color: #659ff5;
}
.main .dayWap .leftLift .on::after {
	display: block;
	content: '';
	width: 0px;
	height: 0px;
	border-top: 4px solid transparent;
	border-left: 4px solid transparent;
	border-bottom: 4px solid #659ff5;
	border-right: 4px solid #659ff5;
	position: absolute;
	top: 50%;
	margin-top: -3px;
	left: -16px;
	transform: rotate(-45deg);
}
.main .dayWap .rightList{
	padding: 40px;

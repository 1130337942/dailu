	font-size: 18px;
	padding-bottom: 10px;
	color: #659ff5;
	position: relative;
}
.main .dayWap .rightList .list .infoBox  .pc::after {
	position: absolute;
	top: 0;
	left: -28px;
	display: block;
	width: 10px;
	height: 10px;
	border-radius: 50%;
	border: 4px solid #659ff5;

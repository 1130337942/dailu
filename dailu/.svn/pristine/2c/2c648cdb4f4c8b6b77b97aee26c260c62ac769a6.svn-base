	padding-left: 35px;
}
.main .costTable .percentBox .p-left{
	width: 500px;
	height: 270px;
	padding-top: 20px;
}
.main .costTable .percentBox .p-right{
	width: 560px;
	height: 270px;
	/*background-color:blue;*/
}
.main .costTable .percentBox .p-right li  {
	width: 50%;
	line-height: 52px;
	/*margin-left: 30px;*/
}
.main .costTable .percentBox .p-right li span {
	vertical-align: middle;
	display: inline-block;

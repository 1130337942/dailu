	padding-bottom: 20px;
	border-bottom: 1px solid #e5e5e5;
}
.main .dayWap .rightList .list .listTit{
	font-size: 20px;
	height: 60px;
	line-height: 60px;
	border-bottom: 1px solid #e5e5e5;
}
.main .dayWap .rightList .list .listTit{
	margin-bottom: 20px;
}
.main .dayWap .rightList .list .listTit .line span{
	font-size: 22px;
	color: #659ff5;
	padding-right: 30px;
	position: relative;
}
.main .dayWap .rightList .list .listTit .line span::after {
	display: block;
	content: '';
	width: 1px;
	height: 14px;
	background-color: #cccccc;
	position: absolute;
	right: 15px;
	top: 50%;
	margin-top: -7px;
}
.main .dayWap .rightList .list .listTit .date .time{
	color: #999;
}
.main .dayWap .rightList .list .infoBox {

	height: 46px;
	text-indent: 2em;
}
.main .costTable table td.t_tit {
	border-left: none ;
}
.main .costTable table td:last-child {
	border-right: none;
}
.main .costTable table td .ico {
	display: inline-block;
	vertical-align: middle;
	width: 20px;
	height: 20px;
	margin-right: 10px;
}
.main .costTable table td .ico.train {
	background: url(/static/v1/img/tripinfo/train.png) center center no-repeat;
}
.main .costTable table td .ico.bus {
	background: url(/static/v1/img/tripinfo/bus.png) center center no-repeat;
}
.main .costTable table td .ico.plane {
	background: url(/static/v1/img/tripinfo/plane.png) center center no-repeat;
}
.main .costTable table td .ico.jd {
	background: url(/static/v1/img/tripinfo/jd.png) center center no-repeat;
}
.main .costTable table td .ico.hotel {
	background: url(/static/v1/img/tripinfo/hotel.png) center center no-repeat;
}
.main .costTable table td .ico.eat {
	background: url(/static/v1/img/tripinfo/eat.png) center center no-repeat;
}

.main .costTable .percent{
	height: 50px;
	line-height: 50px;
	font-size: 20px;
	color: #659ff5;

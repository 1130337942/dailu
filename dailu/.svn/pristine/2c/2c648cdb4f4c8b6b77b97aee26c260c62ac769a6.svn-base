.main .dayWap .rightList .list .infoBox .massage .ms-img i.train {
	background: #fff url(/static/v1/img/tripinfo/train.png) 0 0 no-repeat;
}
.main .dayWap .rightList .list .infoBox .massage .ms-img i.bus {
	background: #fff url(/static/v1/img/tripinfo/bus.png) 0 0 no-repeat;
}
.main .dayWap .rightList .list .infoBox .massage .ms-img i.plane {
	background: #fff url(/static/v1/img/tripinfo/plane.png) 0 0 no-repeat;
}
.main .dayWap .rightList .list .infoBox .massage .msg-txt {
	width: 592px;
	line-height: 26px;
	color:#666;
	height: 100%;

html{
	height: auto;
}
html body {
	min-width: 1200px;
	height: 100%;
	background-color: #f6f6f6;
	color: #333;
	padding-top: 92px;
}
.main {
	width: 1200px;
	margin: 0 auto;
	background-color: #f6f6f6;
}
.hide {
	display: none;
}
.lineBox {
	/*margin-top: 92px;*/
	padding: 30px;
	/*padding-top: 122px;*/
	width: 1138px;
	/*height: 480px;*/
	background-color: #ffffff;
	border-radius: 16px;
	border: solid 1px #f0f0f0;
}
.main .lineBox .leftMsg {
	width: 520px;
	/*background-color: pink;*/
	height: 100%;
}
.main .lineBox .rightPic  {
	margin-top: 10px;
	width: 618px;
	height: 400px;
	border-radius: 16px;
	overflow: hidden;
}
.main .lineBox .leftMsg h4{
	font-size: 24px;
	padding-bottom: 10px;
	position: relative;
	font-weight: 600;
}

.main .lineBox .leftMsg h4::after {
	position: absolute;
	display: block;
	content:'';
	width: 46px;
	height: 4px;
	background-color: #000;
	bottom: 0;
	left: 0;
}
.main .lineBox .leftMsg .place{
	font-size: 17px;
	margin: 10px 0 22px	;
}
.main .lineBox .leftMsg .ico {
	line-height: 30px;
	font-size: 16px;
	color: #666;
	padding-left: 30px;
	position: relative;
}
.main .lineBox .leftMsg .ico::after{
	position: absolute;
	display: block;
	content: '';
	width: 20px;
	height: 20px;
	top: 50%;
	margin-top: -10px;
	left: 0;
}
.main .lineBox .leftMsg .ico.num::after{
	background: #fff url(/static/v1/img/tripinfo/per_num.png) center center no-repeat;
}
.main .lineBox .leftMsg .ico.day::after{
	background: #fff url(/static/v1/img/tripinfo/day_num.png) center center no-repeat;
}
.main .lineBox .leftMsg .ico.startDay::after{
	background: #fff url(/static/v1/img/tripinfo/date.png) center center no-repeat;
}
.main .lineBox .leftMsg .ico.totMoney::after{
	background: #fff url(/static/v1/img/tripinfo/rmb_num.png) center center no-repeat;
}
.main .lineBox .leftMsg .blogger {
	margin-top: 60px;
}
.main .lineBox .leftMsg .blogger .headImg{
	width: 48px;
	height: 48px;
	border-radius: 50%;
	overflow: hidden;
	margin-right: 22px;
}

.main .lineBox .leftMsg .blogger .msg{
	
	height: 48px;
}
.main .lineBox .leftMsg .blogger .name{
	font-size: 18px;
	color: #333;
	
}
.main .lineBox .leftMsg .blogger .msg .date{
	font-size: 14px;
	color: #999;
}
.main .lineBox .leftMsg .shortCart {
	padding-right: 80px;
	padding-top: 20px;
	position: relative;
}
.leftMsg .shortCart  .addTrip{
	width: 180px;
	height: 42px;
	border-radius: 8px;
	border: solid 1px #e5e5e5;
	line-height: 42px;
	color: #999;
	font-size: 14px;
	margin-top: 10px;
	cursor: pointer;
}
.leftMsg .shortCart  .addTrip:hover {
	color: #659ff5;
	font-weight: 600;
}

.leftMsg .shortCart  .addTrip i{
	display: inline-block;
	vertical-align: middle;
	position: relative;
	width: 18px;
	height: 18px;
	background: #fff url(/static/v1/img/tripInfo/addtrip.png) 0 0 no-repeat;
	border-radius: 50%;
	margin: 0 15px;
}
.leftMsg .shortCart  .addTrip i.active {
	background: #fff url(/static/v1/img/tripInfo/addtrip2.png) 0 0 no-repeat;
}

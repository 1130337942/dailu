	font-style: normal;
}
.main .dayWap .rightList .list .infoBox .massage{
	height: 260px;
	
} 
.main .dayWap .rightList .list .infoBox .massage .ms-img{
	width: 300px;
	height: 200px;
	font-size: 14px;
	color: #999;

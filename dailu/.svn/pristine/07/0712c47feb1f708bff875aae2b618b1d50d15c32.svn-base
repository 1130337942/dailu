}
.traffic_more::before {
  content: "";
  width: 0px;
  height: 0px;
  position: absolute;
  left: -18px;
  top: 30px;
  border-color: transparent #FFF transparent transparent;
  border-style: dashed solid dashed dashed;
  border-width: 9px;
  z-index: 2;
}
.traffic_more .air_trc,
.traffic_more .car_trc,
.traffic_more .train_trc {

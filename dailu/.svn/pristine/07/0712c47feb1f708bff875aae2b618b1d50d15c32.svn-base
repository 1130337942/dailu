.imageContainer {
  -ms-scroll-chaining: chained;
  -ms-content-zooming: zoom;
  -ms-scroll-rails: none;
  -ms-content-zoom-limit-min: 100%;
  -ms-content-zoom-limit-max: 500%;
  -ms-scroll-snap-type: mandatory;
  scroll-snap-type: mandatory;
  -ms-scroll-snap-points-x: snapList(100%, 200%, 300%, 400%, 500%);
  -ms-overflow-style: none;
}
::-webkit-scrollbar {
  width: 3px;
}
::-webkit-scrollbar-thumb {
  background-color: rgba(82, 81, 81, 0.3);
  background-clip: padding-box;
  min-height: 28px;
  border-radius: 10px;
}
body {
  background-image: url(/static/v1/img/map/map_bg1.png);
  background-size: 100% 100%;
  cursor: default;
  overflow: hidden;
}
.main_box {
  width: 96%;
  height: 100%;
  margin: 0px auto;
  overflow: hidden;
  position: relative;
}
.content {
  width: 100%;
  height: 87%;
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  top: 95px;
  position: relative;
}
.con_lef_top {
  position: relative;
  height: 13%;
}
.con_lef {
  width: 310px;
  background: #fff;
  height: 100%;
  overflow: hidden;
  position: relative;
  border-radius: 12px;
}
.con_lef .Three_points {
  margin-left: 250px;
  cursor: default;
}
.con_lef .Three_points span {
  display: inline-block;
  width: 8px;
  height: 8px;
  border-radius: 50%;
  margin-top: 16px;
  box-shadow: inset 0px 0px 3px 0px rgba(0, 0, 0, 0.16);
}
.con_lef .Three_points span:nth-child(1) {
  background-color: #a8edfc;
}
.con_lef .Three_points span:nth-child(2) {
  background-color: #e68b1f;
}
.con_lef .Three_points span:nth-child(3) {
  background-color: #ffdf50;
}
.con_lef .f_top {
  margin: 0px 25px 14px 25px;
}
.con_lef .f_top .f_top_p {
  font-size: 18px;
  display: flex;
  align-items: center;
  position: relative;
}
.con_lef .f_top .beyond_day_num {
  color: #e82424;
  font-size: 14px;
  position: absolute;
  margin-top: 6px;
  right: 0;
  top: 0px;
}
.con_lef .f_top .num_day {
  color: #248cd3;
  font-size: 20px;
}
.con_lef .f_top .progressbar_box {
  margin-top: 5px;
  margin-bottom: 35px;
}
.con_lef .f_top #progressbar {
  margin-top: 10px;
}
.con_lef .f_top .calendar {
  font-size: 14px;
  margin-top: -27px;
}
.con_lef .sj_icon {
  margin-left: 5px;
  margin-top: -1px;
}
.con_lef .sj_icon i {
  display: block;
  width: 12px;
  height: 6px;
  cursor: pointer;
}
.con_lef .sj_icon .addsj_icon {
  background: url("/static/common/img/add.png");
}
.con_lef .sj_icon .reducesj_icon {
  margin-top: 3px;
  background: url("/static/common/img/under.png");
}
.con_lef .f_main {
  background-color: #f8f4f4;
  height: 86%;
  overflow: hidden;
  box-shadow: inset 0px 0px 6px 0px rgba(0, 0, 0, 0.15);
  border-radius: 8px;
  position: relative;
  width: 298px;
  margin: 0 auto;
}
.con_lef .f_main:hover {
  overflow: auto;
  -ms-scroll-chaining: chained;
  -ms-content-zooming: zoom;
  -ms-scroll-rails: none;
  -ms-content-zoom-limit-min: 100%;
  -ms-content-zoom-limit-max: 500%;
  -ms-scroll-snap-type: mandatory;
  scroll-snap-type: mandatory;
  -ms-scroll-snap-points-x: snapList(100%, 200%, 300%, 400%, 500%);
  -ms-overflow-style: none;
}
.con_lef .f_main .departure,

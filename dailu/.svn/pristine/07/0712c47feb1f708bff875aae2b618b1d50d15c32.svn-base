}
.f_main_but {
  position: absolute;
  bottom: 12px;
  width: 286px;
  height: 48px;
  line-height: 48px;
  background-image: linear-gradient(90deg, #659ff5 0%, #8fbbfd 100%), linear-gradient(#659ff5, #659ff5);
  background-blend-mode: normal, 
		normal;
  box-shadow: 0px 0px 4px 0px rgba(51, 51, 51, 0.2);
  border-radius: 6px;
  color: #ffffff;
  font-size: 18px;
  text-align: center;
  left: 12px;

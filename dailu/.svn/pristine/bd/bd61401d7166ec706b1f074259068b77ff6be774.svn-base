html,
body {
    width: 100%;
    height: 100%;
    font-family: PingFang SC, Lantinghei SC, Helvetica Neue, Helvetica, Arial, Microsoft YaHei, \\5FAE\8F6F\96C5\9ED1, STHeitiSC-Light, simsun, \\5B8B\4F53, WenQuanYi Zen Hei, WenQuanYi Micro Hei, "sans-serif";
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    text-size-adjust: 100% !important;
    -ms-text-size-adjust: 100% !important;
    -webkit-text-size-adjust: 100% !important;
    cursor: default;
    background-repeat: no-repeat
}

body,
h1,
h2,
h3,
h4,
h5,
h6,
p,
button,
ul,
li {
    margin: 0;
    padding: 0;
}

li {
    list-style: none;
}

a {
    text-decoration: none;
    color: inherit;
}

input,
button,
select,
textarea {
    outline: none;
    -webkit-appearance: none;
    border-radius: 0;
}

img {
    width: 100%;
    height: 100%;
    border: none;
}

.clearfix {
    zoom: 1;
}

.clearfix::after {
    content: "";
    height: 0;
    line-height: 0;
    display: block;
    visibility: hidden;
    clear: both;
}

.dis_none {
    display: none;
}

.fl {
    float: left;
}

.fr {
    float: right;
}

/* 提示框 */

.prompt {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0px;
    left: 0px;
    z-index: 3;
    background-color: rgba(51, 51, 51, 0.68);
}

.prompt_box,
.cancel_prompt_box {
    position: absolute;
    width: 617px;
	height: 342px;
    background: url("/static/common/img/prompt.png");
    /* border-radius: 24px; */
    top: calc(50% - 171px);
    left: calc(50% - 308.5px);
    font-size: 14px;
    /* box-shadow: inset 0px 0px 6px 0px rgba(0, 0, 0, 0.2); */
    z-index: 3;
    color: #659ff5;
    /* overflow: hidden; */
}
.prompt_content{
    width: 320px;
    float: right;
    margin-top: 95px;
    overflow: hidden;
}
.prompt_title {
    font-size: 28px;
    font-weight: bold;
}

.prompt_text,
.cancel_prompt_text {
    color: #659ff5;
    margin-top: 10px;
    font-size: 16px;
    width: 250px;
}

.prompt_but,.prompt_det,.prompt_cancel {
    padding: 0 15px;
	height: 28px;
    color: #ffffff;
    margin-top: 20px;
    line-height: 28px;
    cursor: pointer;
    background-color: #649ff5;
	box-shadow:0px 2px 8px 0px
		rgba(101, 159, 245, 0.4);
    border-radius: 14px;
    text-align: center;
    float: left;
}
.prompt_cancel{
    color: #999999;
    background-color: #e5e5e5;
    box-shadow: none;
}
.prompt_but:hover,.prompt_det:hover{
    background-color: #74adff;
}
.prompt_but:active,.prompt_det:active,.prompt_cancel:active{
    background-color: #5a91e1;
}
.prompt_cancel:hover{
    background-color: #e9e9e9;
}
.cancel_prompt_but{
    overflow: hidden;
}

.cancel_prompt_but div:nth-child(1){
    margin-right: 12px;
    margin-bottom: 10px;
}
.prompt_but{
    margin-bottom: 10px;
}


/* 导览 */

.visitors {
    z-index: 2;
    position: absolute;
    background-image: linear-gradient(0deg, #f4a21e 0%,#fac063 100%);
    background-blend-mode: normal, normal;
    box-shadow: 0px 2px 10px 0px rgba(0, 0, 0, 0.1);
    border-radius: 5px;
    text-shadow: 0px 2px 2px rgba(0, 0, 0, 0.1);
}

.visitors::before {
    content: "";
    position: absolute;
    width: 0px;
    height: 0px;
    overflow: hidden;
    border-width: 10px;
}

.visitors_text {
    color: #ffffff;
    font-size: 18px;
    margin: 15px 15px 0px 15px;
}

.visitors_end {
    color: #ffffff;
    font-size: 12px;
    position: absolute;
    right: 15px;
    bottom: 10px;
    cursor: pointer;
}

.gm-style-iw div {
    overflow: inherit !important;
}

.loading_box {
    position: fixed;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
    background-color: rgba(255, 255, 255, 0.9);
    z-index: 99;
}

.loading_box .loading_content {
    background-color: #fff;
    width: 400px;
    height: 250px;
    position: absolute;
    left: calc(50% - 200px);
    top: calc(50% - 125px);
    border-radius: 12px;
    box-shadow: 0px 2px 16px 0px rgba(0, 0, 0, 0.1);
}

.loading_box img {
    margin-top: 50px;
    margin-left: calc(200px - 60px);
    width: 120px;
    height: 120px;
}

.loading_box .loading_text {
    text-align: center;
    color: #666;
    font-size: 16px;
}
#map .info{
    background: #fff;
    font-size: 14px;
    padding:3px 15px;
    border-radius: 5px;
    box-shadow: 0px 1px 4px 0px rgba(51, 51, 51, 0.2);
    color: #333333;
}

#map .info::before{
    content: "";
    background: url("/static/v1/img/map/infobox.png");
    position: absolute;
    bottom: -8px;
    left: calc(50% - 6px);
    width: 12px;
    height: 8px;
}
.info_text_none{
    padding:3px 15px;
    display: none;
    font-size: 14px;
    border-radius: 5px;
    box-shadow: 0px 1px 4px 0px rgba(51, 51, 51, 0.2);
}
.map_head_r {
    float: right;
    margin-right: 50px;
    color: #454545; 
}
.map_head_r i {
    display: inline-block;
    margin-bottom: -3px;
    margin-right: 5px;
}
.map_head_r span{
    cursor: pointer;
}
.map_head_r .icon_1 {
    width: 19px;
    height: 16px;
    background-image: url("/static/v1/img/map/onlineserver.png");
}
.map_head_r .icon_2 {
    width: 14px;
    height: 16px;
    background-image: url("/static/v1/img/map/helpicon.png");
    margin-left: 20px;
}
	left:502px;
	z-index: 10;
}

/* 弹窗轮播图 start*/
#slideshow {
	display: none;
	position: fixed;
	top: 0;
	left: 0;
	background-color: rgba(0,0,0,0.8);
	width: 100%;
	height: 100%;
	z-index: 99	;		
}
	

#slideshow .topWap {
	height: 500px;
	position: relative;
	width: 1200px;
	margin: 0 auto;
	margin-top: 5%;
}
#slideshow .topWap .prevBtn,
#slideshow .topWap .nextBtn{
	position: absolute;
	display: block;
	top: 50%;
	margin-top: -35px;	
	width: 36px;
	height: 70px;
	cursor: pointer;
}

.slider_close{
	width: 36px;
	height: 36px;
	background:  url(/static/v1/img/slider_close.png) center center no-repeat;
	border-radius: 50%;
	position: absolute;
	top: 0;
	right: 0;	
	cursor: pointer;
	opacity: 0.6;
	transition: all 0.2s;
}
.slider_close:hover{
	opacity: 1;
}

#slideshow .topWap .prevBtn::before,
#slideshow .topWap .nextBtn::before{
	display: block;
	content: '';
	width: 49px;
	height: 49px;
	border: 1.5px solid #fff;
	border-right: none;
	border-bottom: none;
	transform:rotate(-45deg);
	opacity: 0.6;
	transition: all 0.2s;
}

#slideshow .topWap .prevBtn:hover::before,
#slideshow .topWap .nextBtn:hover::before{
	opacity: 1;
}

#slideshow .topWap .nextBtn::before{
	transform:rotate(135deg);
}
#slideshow .topWap .prevBtn{
	left: 0px;
}
#slideshow .topWap .nextBtn{
	right: 0px;
}
#slideshow .topWap .box_bigimg {
	width: 718px;
	height: 500px;
	text-align: center;
	margin: 0 auto;
	overflow: hidden;
}
#slideshow  .box_bigimg img {
	width: auto;
	height: 100%;
} 

#slideshow .botomWap{
	margin-top: 120px;
	width: 100%;
	height:180px;
	background-color: rgba(0,0,0,0.8); 
}
#slideshow .botomWap .response_wap{
	width: 1200px;
	margin: 0 auto;
	position: relative;
}
#slideshow .botomWap .imglist_prev,
#slideshow .botomWap .imglist_next{
	position: absolute;
	width: 24px;
	height: 100px;
	background-color:rgba(0,0,0,0.5);
	bottom: 0;
	cursor: pointer;
}

#slideshow .botomWap .imglist_prev::before,
#slideshow .botomWap .imglist_next::before{
	/*position: absolute;*/
	display: block;
	content: '';
	width: 10px;
	height: 10px;
	border: 2px solid #e5e5e5;
	border-right: none;
	border-top: none;
	opacity: 0.86;
	transform: rotate(45deg);
	margin-top: 40px;
}
#slideshow .botomWap .imglist_prev::before{
	margin-left: 8px;
}
#slideshow .botomWap .imglist_next::before {
	transform: rotate(-135deg);
	margin-left: 3px;
}

#slideshow .botomWap .imglist_prev{
	left: 0;
}
#slideshow .botomWap .imglist_next{
	right:0;
}

#slideshow  .img_filter {
	height: 36px;
}
.response_wap .imglist{
	height: 100px;
	overflow: hidden;
}
.response_wap .imglist ul {
	position: relative;
	transition: all 0.3s;
}
.response_wap .imglist li:first-child{
	margin-left: 0;
}
.response_wap .imglist li.active {
	border: 3px solid #569ff5;
}
.response_wap .imglist li {
	width: 109px;
	height: 100px;
	margin-left: 12px;
	overflow: hidden;
	box-sizing: border-box;
	cursor: pointer;
}

.response_wap .imglist li img {
	width: 100%;
	height:auto;
}

/* 弹窗轮播图 end*/
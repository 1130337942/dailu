html,body {
	height: 100%;
}
.hide {
	display: none;
}
.main {
	width: 100%;
	/*height: 100%;*/
	background: #fff url(/static/v1/img/share_bg.jpg) 0 0 repeat-y;
}

.main .title {
	background-color: #fff;
	margin: 0 auto;
	width: 1156px;
	height: 50px;
	line-height: 50px;
	box-shadow: 0px 1px 8px 0px 
		rgba(0, 0, 0, 0.08);
	border-radius: 8px;
	font-weight: 600;
	color: #999;
	padding:0 12px 0 32px;
}
.main .title .left_tab span{
	display: inline-block;
	height: 47px;
	padding:0 3px;
	margin-right: 15px;
	cursor: pointer;
}
.main .title .left_tab span.active,
.main .title .left_tab span:hover {
	color: #659ff5;
	border-bottom: 3px solid #659ff5;
}
.main .title .right_ctrl a{
	margin-top: 10px;
	text-align: center;
	display: inline-block;
	width: 80px;
	height: 30px;
	line-height: 30px;
	background-color: #659ff5;
	border-radius: 4px;
	color: #fff;
}
.main .title .right_ctrl a:hover {
	background: #4586e7;
}
.book_box {
	border:1px solid transparent;
	margin:0 auto;
	width: 1175px;
	height: 803px;
	background: url(/static/v1/img/book.png) center center no-repeat;
	position: relative;
}
.book_box .book_outer{
	border-radius: 16px;
	width: 1100px;
	height: 656px;
	margin-top: 60px;
	margin-left: 39px;
	-webkit-user-select:none;
    -moz-user-select:none;
    -ms-user-select:none;
    user-select:none;
    overflow: hidden;
}
/*#flipbook .page-wrapper:nth-child(2n+1) {
	background-color: #ccc;
}*/

/*flipbook style*/

.book_box .book_outer #flipbook > div{
	border-radius: 16px;
}
#flipbook .page_inner {
	border-radius: 16px;
	padding:0 28px;
}
#flipbook .page_head {
	line-height: 40px;
	font-size: 14px;
	color: #999;
	font-weight: 600;
	border-bottom: 1px solid #ccc;
}
#flipbook .hard {
	border-radius: 16px;
}
.hard_tit {
	color: #666;
	border-bottom: 1px solid #659ff5;
	padding-bottom: 5px;
	margin-bottom: 20px;
}
.hard_tit .book_title {
	color: #659ff5;
	font-size: 24px;
	font-weight: 600;
}
#flipbook .catalog {
	position: relative;
	line-height: 36px;
	color: #666;
}
#flipbook .catalog .cat_con {
	padding-left: 28px;
	position:relative;
	z-index: 3;
}
#flipbook .catalog::after {
	position: absolute;
	top: 50%;
	right: 120px;
	display: block;
	content: '';
	height: 0;
	width: 300px;
	border-bottom: 1px dashed #ccc;
	z-index: 2;
}
#flipbook .catalog .cat_con {
	font-weight: 600;
}

.page {
	overflow-y: auto;
	background-color: #fff;

}
#flipbook .even {
	background-color: rgb(240, 240, 240);
}
.page::-webkit-scrollbar {
  width: 5px;
}

.page::-webkit-scrollbar-thumb {
  background-color: #659ff5;
  background-clip: padding-box;
  min-height: 28px;
  border-radius: 10px;
}
/*page start*/
.page_tit {
	/*text-align: right;*/
	color: #666;
	border-bottom: 1px solid #659ff5;
	padding-bottom: 5px;
	margin-bottom: 20px;
}
.page_tit .book_title {
	color: #659ff5;
	font-size: 24px;
	font-weight: 600;
}

img {
	width:500px;
	height: 300px;
}

.page .city_catalog{
	font-size: 18px;
	line-height: 40px;
	position: relative;
}
.page .city_catalog .zxq_ico{
	font-style: normal;
	display: inline-block;
	width: 16px;
	height: 16px;
	background-color: #659ff5;
	border-radius: 50%;
	color: #fff;
	text-align: center;
	line-height: 16px;
	font-size: 12px;
	position: relative;
}
.page .city_catalog .zxq_ico::before{
	display: block;
	content: '';
	width: 2px;
	height: 20px;
	background-color: #e5e5e5;
	position: absolute;
	left: 7px;
	top:19px;
}
.page .city_catalog:last-child .zxq_ico::before{
	display: none;
}
.page .city_catalog::before {
	display: block;
	content: '';
	width: 200px;
	height: 0;
	border-bottom: 1px dashed #ccc;
	position: absolute;
	top: 50%;
	left: 140px;
}
.page .city_catalog_box  .city_catalog:first-child .day,
.page .city_catalog_box  .city_catalog:last-child .day{
	color: #999;
}
.page .city_catalog_box  .city_catalog:last-child .zxq_ico{
	background-color: #f4a21e;
}
.page .city_catalog_box  .city_catalog .day {
	font-size: 16px;
}
.page .city_catalog_box .city_catalog:first-child .zxq_ico::after,
.page .city_catalog_box  .city_catalog:last-child .zxq_ico::after{
	display: block; 
	content: '';
	width: 10px;
	height: 10px;
	border-radius: 50%;
	background-color: #fff;
	position: absolute;
	top: 3px;
	left: 3px;                       
}
.page .page_footer{
	line-height: 46px;
	color: #999;
	font-size: 12px;
	text-align: center;
}
.hard_last{
	background-color: #fff;
}

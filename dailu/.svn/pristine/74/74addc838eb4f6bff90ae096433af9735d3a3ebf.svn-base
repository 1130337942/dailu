htmlï¼Œbody {
	background-color: #535257;
	height: 100%;
}
html ,body ,.main {
	min-height: 100%;	
}
.hide {
	display: none;
}
.main {	
	background-color: #fff;
	max-width: 4.14rem;
	margin:0 auto;
	box-sizing: border-box;
	color:#333;
	font-family: "PingFang","Lantinghei","Helvetica Neue",'Helvetica','Arial',"Microsoft YaHei","STHeitiSC-Light",'simsun',"WenQuanYi Zen Hei","WenQuanYi Micro Hei","sans-serif";
	font-size: 0.14rem;
}
.p18{
	padding: 0.18rem;
}
.main .header {
	font-size: 18px;
	color: #333;
	height: 0.44rem;
	width: 100%;
	line-height: 0.44rem;
	text-align: center;
	/*border-bottom: 1px solid #e5e5e5;*/
	 position: absolute;
	top:0;
	left: 0; 
	z-index: 9;
	background-image: linear-gradient(180deg, rgba(0,0,0,0.3) 0% ,rgba(0,0,0,0) 100%);
}
.main .header span{
	position: absolute;
	left: 0.2rem;
	top: 0.14rem;
	width: 0.12rem;
	height: 0.12rem;
	transform:  rotate(45deg);
	border: 0.02rem solid #fff;
	border-right: none;
	border-top: none;
}
.main .banner_img {
	width: 100%;
	height: 2.5rem;	
	position: relative;
}
/*.main .banner_img img{
	width: 100%;
	height: 100%;
	position:absolute;
	top:0;
	left:0;
}*/
.main .banner_img img{

}
.tabs {
	background-color: #fff;
	position: absolute;
	bottom: 0.3rem;
	left: 50%;
	transform: translateX(-50%);
	z-index: 11;
	border-radius: 4px;
	display: flex;
	justify-content: space-between;
	overflow:hidden;
}
.tabs span {
	width: 0.4rem;
	height: 0.28rem;
	display: inline-block;
	text-align: center;
	line-height: 0.28rem;
}
.tabs span.active {
	background-color: #ffd913;
}
.banner_img .imgCount {
	width: 0.38rem;
	height: 0.2rem;
	background-color: #000000;
	border-radius: 0.02rem;
	font-size: 0.12rem;
	color: #fff;
	opacity: 0.6;
	text-align: center;
	line-height: 0.2rem;
	position: absolute;
	bottom: 0.2rem;
	right: 0.2rem;
	z-index: 9;
}
.detail_info{
	position: relative;
	background-color: #fff;
	border-radius: 0.12rem;
	padding:0.2rem 0.18rem; 
	margin-top: -0.14rem;
	z-index: 2;
	color: #333;
}
.detail_info .detail_tit{
	font-size: 0.2rem;
	font-weight: 600;
}
.detail_info .address {
	font-size: 0.14rem;
	color: #666;
	height: 0.34rem;
	line-height: 0.24rem;
	white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
	border-bottom: 0.01rem solid #e5e5e5;
}
.detail_info .itemlist span{
	color:#b3b3b3;
	margin-right: 0.25rem;
}

.detail_info .itemlist .ticket span ,
.detail_info .itemlist .other_desc span {
	margin: 0;
	color: #333;
}
.detail_info .itemlist  .spot_desc,
.detail_info .itemlist  .ticket,
.detail_info .itemlist  .other_desc {
	display: block;   
	max-height: 0.6rem;
}
.detail_info .itemlist strong{
	font-weight:normal;
	
}
.detail_info .itemlist {
    overflow: hidden;
    margin-top: 0.1rem;
	position: relative;
}
.detail_info .itemlist .more_msg {
	position: absolute;
	bottom: 0;
	right: 0;
	color: #659ff5;
	font-size: 0.12rem;
	background-color: #fff;
}
.detail_info .itemlist .more_msg.up::before{
	display: none;
}
.detail_info .itemlist .more_msg::before {
	display: inline-block;
	vertical-align: middle;
	content: '...';
	color:#659ff5;
	margin-right: 0.05rem;
}
.detail_info .itemlist .ico{
	margin-right: 0.05rem;
	display: inline-block;
	vertical-align: middle;
	width: 0.18rem;
	height: 0.18rem;
	background: url(/static/v1/img/tripinfoShare/leixing.png) center center no-repeat;
	-webkit-background-size: 100% 100%;
	background-size: 100% 100%;
}
.detail_info .itemlist .ico.shiwan{
	background: url(/static/v1/img/tripinfoShare/shiwan.png) center center no-repeat;
	-webkit-background-size: 100% 100%;
	background-size: 100% 100%;
}
.detail_info .itemlist .ico.kaifang{
	background: url(/static/v1/img/tripinfoShare/kaifang.png) center center no-repeat;
	-webkit-background-size: 100% 100%;
	background-size: 100% 100%;
}
.detail_info .itemlist .ico.jijie{
	background: url(/static/v1/img/tripinfoShare/jijie.png) center center no-repeat;
	-webkit-background-size: 100% 100%;
	background-size: 100% 100%;
}
.detail_info .itemlist .ico.dianhua{
	background: url(/static/v1/img/tripinfoShare/dianhua.png) center center no-repeat;
	-webkit-background-size: 100% 100%;
	background-size: 100% 100%;
}
.detail_info .itemlist .ico.menpiao{
	background: url(/static/v1/img/tripinfoShare/menpiao.png) center center no-repeat;
	-webkit-background-size: 100% 100%;
	background-size: 100% 100%;
}
.detail_info .itemlist .ico.dizhi{
	background: url(/static/v1/img/tripinfoShare/dizhi.png) center center no-repeat;
	-webkit-background-size: 100% 100%;
	background-size: 100% 100%;
}
.detail_info .itemlist .ico.tebie{
	background: url(/static/v1/img/tripinfoShare/tebie.png) center center no-repeat;
	-webkit-background-size: 100% 100%;
	background-size: 100% 100%;
}
.detail_info .itemlist  a {
	text-decoration: underline;
	color: #659ff5;
}
.comment_wap{
	padding: 0.2rem 0.18rem;
}
.comment_wap .title {
	font-size: 0.18rem;
	position: relative;
	border-top: 1px solid #e5e5e5;
	padding-top: 0.15rem;
}
.comment_wap .commemt_box {
	background-color: #fff;
	/*border-radius: 8px;*/
	margin-bottom: 0.28rem;

}
.comment_wap .toComment {
	width: 0.32rem;
	height: 0.32rem;
	display: block;
	position: absolute;
	top: 50%;
	right: 0.2rem;
	margin-top: -0.16rem;
	background: url(/static/v1/img/tripinfoShare/tocomment.png) center center no-repeat;
	-webkit-background-size: 50% 50%;
	background-size: 50% 50%;
}
.comment_list{
	box-sizing: border-box;
	padding: 0.16rem 0;
	font-size: 14px;
	color: #666;
	text-align: left;
	border-bottom: 1px solid #e5e5e5;
}
.comment_list:last-child{
	border-bottom: none;
}
.comment_list .con {
	padding-left: 0.44rem;
}
.comment_list .uploadTime{
	font-size: 0.12rem;
	color: #b3b3b3;
	padding-left: 0.44rem;
	margin-top: 0.1rem;
}
.comment_list .head {
	display: flex;
}
.comment_list .head img {
	width: 0.34rem;
	height: 0.34rem;
	border-radius: 50%;	
}

.comment_list .head span {
	line-height: 0.34rem;
	font-size: 0.14rem;
	color: #333;
	margin-left: 0.1rem;
}
.comment_list .uploadImg {
	overflow: hidden;
	display: flex;
	margin-top: 0.05rem;
	padding-left: 0.4rem;
}
.comment_list .uploadImg img {
	width: 0.7rem;
	height: 0.7rem;
	margin-left: 0.05rem;
}
.comment_in{
	border-radius: 8px;
	/*height: 2rem;*/
}
.commemt_box .morecomment,
.commemt_box .nocomment {
	text-align: center;
	font-size: 0.14rem;
	color: #999;
	height: 0.4rem;
	line-height: 0.4rem;
	display: none;
}
.commemt_box .morecomment{
	display: block;
}

.sliderMask  img {
	width: 100%;
	/*box-sizing: border-box;*/
}
.sliderMask .sliderIMg{
	width: 100%;
} 
.sliderbox {
	z-index: 12;
	position: fixed;
	top:0;
	left:0;
	width: 100%;
	height: 100%;
	background-color: rgba(0,0,0,1);
	display: none;
}
.sliderMask {
	width: 100%;
	position: absolute;
	left: 0;
	top: 50%;	
	transform: translateY(-50%);
}
.swiper-container1 {
	overflow:hidden;	
}
.swiper-container2 {
	width: 100%;
	height: 100%;
	overflow:hidden;
}
.swiper-container2 .swiper-wrapper{
	width: 100%;
	height: 100%;
}
.swiper-container2 .swiper-wrapper .swiper-slide{
	width: 100%;
	height: 100%;
	background-color: rgba(0,0,0,0.8);
}
.swiper-container2 {

} 
.swiper-container2 .poster{
	position: absolute;
	top: 0;
	left:0;
	width: 100%;
	height: 100%;
	z-index: 3;
	border: none;
} 
video {
	height: 100%;
	width: 100%;
	display: none;
}
.banner_img .video_play{
	width: 0.4rem;
	height: 0.4rem;
	border-radius: 50%;
	background: #fff url(/static/v1/img/video_play.png) center center no-repeat;
	-webkit-background-size: 100% 100%;
	background-size: 100% 100%;
	display: none;
	position: absolute;
	top: 40%;
	left: 50%;
	z-index: 11;
	transform: translate( -50%,-50%);
}	
	display: block;
	width:37px;
	height:36px;
	text-align: center;
	line-height: 36px;
	border-right: 1px solid #e5e5e5;
	font-size: 14px;
	color: #999;
	transition: all 0.3s;
}
.cityBar .cityAll:hover,
.cityBar .citycon .city:hover,
.cityBar .citycon .daySourse:hover,
.cityBar .cityAll.active,
.cityBar .citycon .city.active,
.cityBar .citycon .daySourse.active{
	background:rgba(101,159,245,1);
	border-bottom: 1px solid #fff;
	color:#fff;
	border-right-color:transparent;
}
.cityBar .citycon .city{
	color: #999;
	width:100%;
	height:37px;
	text-align: center;
	line-height: 36px;
	border-bottom: 1px solid #e5e5e5;
	border-right: 1px solid #e5e5e5;
	box-sizing: border-box;
	transition: all 0.3s;
}
.citycon .citylist:last-child .city,
.citycon .citylist:last-child .daySourse:last-child{
	border-right: none;
} 
.mapctrl span {
	width: 50%;
	height: 74px;
	position: absolute;
	top: 0;
}
.mapctrl span.prev{
	left:0;
} 
.mapctrl span.prev::before {
	display: block;
	content: '';
	width: 0;
	height:0;
	border:3px solid #333;
	position: absolute;
	top: 48%;
	left: 60%;
	transform: rotate(45deg);
	border-right-color: transparent;
	border-top-color: transparent;
}
.mapctrl span.next{
	right: 0;
} 
.mapctrl span.next:hover,
.mapctrl span.prev:hover{
	background-color: #faf9f9;
}
.mapctrl span.next::before {
	display: block;
	content: '';
	width: 0;
	height:0;
	border:3px solid #333;
	position: absolute;
	top: 48%;
	right: 60%;
	transform: rotate(45deg);
	border-bottom-color: transparent;
	border-left-color: transparent;
}


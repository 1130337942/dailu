html,body {
	height: 100%;
}
.hide {
	display: none;
}
.main {
	width: 100%;
	/*height: 100%;*/
	background: #fff url(/static/v1/img/share_bg.jpg) 0 0 repeat-y;
}
body .header{
	z-index: 10;
}
.main .title {
	background-color: #fff;
	margin: 0 auto;
	width: 1167px;
	height: 50px;
	line-height: 50px;
	box-shadow: 0px 1px 8px 0px 
		rgba(0, 0, 0, 0.08);
	border-radius: 8px;
	font-weight: 600;
	color: #666;
	padding-left:32px;
	overflow:hidden;
}
.main .title .left_tab span{
	display: inline-block;
	height: 47px;
	padding:0 3px;
	margin-right: 15px;
	cursor: pointer;
	border-bottom: 3px solid transparent;
	transition: all 0.2s;
}
.main .title .left_tab span.active,
.main .title .left_tab span:hover {
	color: #659ff5;
	border-bottom: 3px solid #659ff5;
}
.main .title .right_ctrl{
	overflow: hidden;
	border-radius: 0 4px 4px 0;
}
.main .title .right_ctrl a{
	font-size: 14px;
	color:#666;
	cursor: pointer;
	padding:0 20px;
	transition: all 0.3s;

}
.main .title .right_ctrl a:hover {
	color: #659ff5;
	text-decoration: underline;
}
.main .title .right_ctrl a::before {
	display: inline-block;
	vertical-align: middle;
	width: 16px;
	height: 16px;
	content: '';
	margin-right: 10px;
	padding-bottom: 3px;
	background:  url(/static/v1/img/tripinfo/fb.png) 0 top no-repeat;
}
/*.main .title .right_ctrl a.issue{
	border-left: 1px solid #e5e5e5;
}*/
.main .title .right_ctrl a.editTrip::before{
	background:  url(/static/v1/img/tripinfo/xg.png) 0 top no-repeat;
}
.main .title .right_ctrl .fabu{
	width: 110px;
	height: 50px;
	border-radius: 0px 4px 4px 0px;
	line-height: 50px;
	text-align: center;
	color: #fff;
	font-size: 16px;
	display: inline-block;
	font-weight: 600;
	transition: all 0.2s;
	cursor: pointer;
	margin-right: -3px;
	padding: 0;
	background-color: #ccc;
}
.main .title .right_ctrl .issue{
	background-color: #78acfa;
}
.main .title .right_ctrl  .issue:hover{
	background-color: #74adff;
}
.book_box {
	border:1px solid transparent;
	margin:0 auto;
	width: 1175px;
	height: 803px;
	background: url(/static/v1/img/book.png) center center no-repeat;
	position: relative;
}
.book_box .book_outer{
	border-radius: 16px;
	width: 1100px;
	height: 656px;
	margin-top: 60px;
	margin-left: 39px;
	-webkit-user-select:none;
    -moz-user-select:none;
    -ms-user-select:none;
    user-select:none;
    overflow: hidden;
}

.spotmsg .title{
	margin-bottom: 10px;
	width: 314px;
	height: 46px;
	line-height: 46px;
	text-indent: 2em;
	background-color: #20d9ac;
	color: #fff;
	padding-right: 46px;
	text-overflow:ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 1;
	overflow:hidden;

}
.spotmsg .title .close {
	transition: all 0.3s;
	width: 46px;
	height: 46px;
	text-align: center;
	position: absolute;
	top:0;
	right:0;
	cursor: pointer;
	text-indent:0em;
}
.spotmsg .title .close:hover {
	transform: rotate(180deg);
}
.spotmsg .item {
	line-height: 28px;
	color: #666;
	text-indent: 2em;
	text-overflow:ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;

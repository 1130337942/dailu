input {
	outline:none;
	appearance:none; 
	border:none;
	font-family: 'DINMittelschriftStd';
}
.hide{
	display: none;
}
.main{
	width: 1200px;
	margin: 0 auto;
}
.bread {
	margin-top: 98px;
	font-size: 14px;
	height: 36px;
	line-height: 36px;
	color:#666;
}
.bread .link {
	color: #659ff5;
}
.hetel_show {
	padding: 24px 26px;
	margin: 0 auto;  
	width: 1148px;
	background-color: #fff;
	box-shadow: 0px 0px 10px 0px 
		rgba(0, 0, 0, 0.04);
	border-radius: 4px;
}

.hetel_show .big_img{
	width: 390px;
	height: 300px;
	border-radius: 4px;
	overflow: hidden;
}
.hetel_show .right_info{
	width: 715px;
	height: 300px;
}
.hetel_show .right_info .title {
	position: relative;
}
.hetel_show .right_info .title .tit {
	font-size: 24px;
	font-family: PingFang-SC-Bold;
}
.hetel_show .right_info .title .loca {
	margin: 5px 0 ;
	color: #999;
	font-size: 14px;
}
.hetel_show .right_info .title .loca span {
	color: #659ff5;
}
.hetel_show .right_info .title .service{
	margin-bottom: 5px;
}
.hetel_show .right_info .title .service .ico{
	width: 22px;
    height: 22px;
    display: inline-block;
    margin-right: 5px;
}
.hetel_show .right_info .title .service .ico.ico1,
.hetel_show .right_info .title .service .ico.ico2,
.hetel_show .right_info .title .service .ico.ico3,
.hetel_show .right_info .title .service .ico.ico4{
	background: #fff url(/static/v1/img/hotelServer_ico.png) -56px 0 no-repeat;
}
.hetel_show .right_info .title .service .ico.ico5,
.hetel_show .right_info .title .service .ico.ico6{
	background: #fff url(/static/v1/img/hotelServer_ico.png) -28px 0 no-repeat;
}
.hetel_show .right_info .title .service .ico.ico7,
.hetel_show .right_info .title .service .ico.ico8{
	background: #fff url(/static/v1/img/hotelServer_ico.png) -168px 0 no-repeat;
}
.hetel_show .right_info .title .service .ico.ico9,
.hetel_show .right_info .title .service .ico.ico10{
	background: #fff url(/static/v1/img/hotelServer_ico.png) -112px 0 no-repeat;
}

.hetel_show .right_info .title .service .ico.ico11{
	background: #fff url(/static/v1/img/hotelServer_ico.png) -84px 0 no-repeat;
}
.hetel_show .right_info .title .service .ico.ico12,
.hetel_show .right_info .title .service .ico.ico13{
	background: #fff url(/static/v1/img/hotelServer_ico.png) -140px 0 no-repeat;
}
.hetel_show .right_info .title .service .ico.ico14{
	background: #fff url(/static/v1/img/hotelServer_ico.png) -224px 0 no-repeat;
}
.hetel_show .right_info .title .service .ico.ico15{
	background: #fff url(/static/v1/img/hotelServer_ico.png) -196px 0 no-repeat;
}
.hetel_show .right_info .title .service .ico.ico16{
	background: #fff url(/static/v1/img/hotelServer_ico.png) 0px 0 no-repeat;
}
.hetel_show .right_info .title  .grade{
	position: absolute;
	top: 0;
	right: 0;
	text-align: center;
}
.hetel_show .right_info .title  .grade span{
	width: 45px;
	display: inline-block;
	/*width: 95px;*/
}
.hetel_show .right_info .title  .grade p:first-child {
	font-family: PingFang-SC-Bold;
}
.hetel_show .right_info .bottom_info .left_img {
	width: 408px;
}
.hetel_show .right_info .bottom_info .img_list {
	width: 128px;
	height: 98px;
	margin-right: 8px;
}
.hetel_show .right_info .bottom_info .img_list:nth-child(n+4){
	margin-top: 8px;
}
.hetel_show .right_info .bottom_info .map{
	width: 280px;
	height: 204px;
	/*background-color: pink;*/
}
#map_s{
	width: 100%;
	height: 100%;
}
.title_bar {
	padding:0 33px;
	width: 1134px;
	margin: 8px auto 0;
	background-color: #fff;
	height: 56px;
	line-height: 56px;
	border-bottom: 1px solid #e5e5e5;
	color: #999;
	font-size: 14px;
}
.title_bar a {
	font-size: 18px;
	display: inline-block;
}

.title_bar .tab {
	margin-right: 20px;
	height: 54px;
	cursor: pointer;
}

.title_bar .searchBtn{
	border:none;
	padding: 0 10px;
	height: 32px;
	line-height: 32px;
	border-radius: 4px;
	background-color: #659ff5 ;
	color: #fff;
	cursor: pointer;
}
.title_bar .searchBtn::hover {
	background-color: #5395f7;
}
.title_bar .tab:hover,
.title_bar .tab.active{
	color: #659ff5;
	border-bottom:3px solid #659ff5;
}
.title_bar span {
	margin-left: 20px;
}
.title_bar span input {
	margin-left: 5px;
	padding-left: 12px;
	width: 100px;
	height: 30px;
	border-radius: 4px;
	border: solid 1px #e5e5e5;
}
.hotel_list{
	width: 1200px;
	background-color: #fff;
	margin: 0 auto;
}
.hotel_list .list {
	padding: 27px 33px;
	border-bottom: 1px solid #e5e5e5;
}
.hotel_list .list:hover {
	/*background-color: #fbfbfb;*/
}
.hotel_list .list .tit {
	margin-bottom: 5px;
	font-weight: 600;
}
.hotel_list .list .service {
	font-size: 14px;
	color: #999;
}
.hotel_list .list .service span {
	position: relative;

}
.hotel_list .list .service span::after {
	display: inline-block;
	content: '';
	width: 1px;
	height: 13px;
	background-color: #999;
	vertical-align: middle;
	margin:0 10px;
}
.hotel_list .list .service span:last-child::after{
	display: none;
}
.hotel_list .list .preview{
	position: relative;
}
.hotel_list .list .preview .short {
	position: absolute;
	height: 40px;
	line-height: 40px;
	top: 50%;
	margin-top: -20px;
	right: 0;

}

.hotel_list .list .preview .short .showBtn {
	display: inline-block;
	line-height: 38px;
	color: #fff;
	width: 116px;
	height: 38px;
	background-color: #659ff5;
	border-radius: 4px;
	text-align: center;
}
.hotel_list .list .preview .short .showBtn::after{
	display: inline-block;
	content: '';
	width: 0;
	height: 0;
	border-right: 4px solid #fff;
	border-bottom: 4px solid #fff;
	border-top: 4px solid transparent;
	border-left: 4px solid transparent;
	transform: rotate(45deg);
	margin-left: 8px;
	margin-bottom: 5px;
}
.hotel_list .list .preview .short .price {
	font-size: 28px;
	font-family: 'DINMittelschriftStd';
	color: #f4a21e;
	font-style: normal;
	margin-right: 58px;
}
.hotel_list .list .preview .short .price em {
	font-size: 14px;
	font-style: normal;
}
.hotel_list .list .slideWap {
	margin-top: 27px;
	width: 1070px;
	/*height: 400px;*/
	padding:0 32px;
	background-color: #fafafa;
}
.hotel_list .list .slideWap .slide_title {
	font-size: 14px;
	height: 43px;
	line-height: 43px;
	border-bottom: 1px solid #e5e5e5;
	color: #999;
}
.hotel_list .list  .slideWap span {
	display: inline-block;
}
.hotel_list .list  .slideWap .cp {
	width: 254px;
}
.hotel_list .list  .slideWap .gy {
	width: 172px;
}
.hotel_list .list  .slideWap .zc {
	width: 120px;
}
.hotel_list .list  .slideWap .qx {
	width: 174px;
}

.hotel_list .list  .slideWap .rj {
	width: 162px;
}
.hotel_list .list  .slideWap .zt {

	width: 160px;
}
.hotel_list .list  .slideWap .price {
	color: #f4a21e;
	font-family: 'DINMittelschriftStd';
	font-weight: 500;
	font-size: 16px;
}
.hotel_list .list  .slideWap .zt.can{
	color: #659ff5;
	font-size: 16px;
}
.hotel_list .list  .slideWap .zt.can span{
	color:#f97956;
	font-size: 14px;
}
.hotel_list .list  .slideWap .zt.can:hover{
	color: #3881ee;
}
.hotel_list .list  .slideWap .zt.not{
	color: #cccccc;
}
.hotel_list .list  .slideWap .zt a {
	text-decoration: underline;
}
.hotel_list .list  .slideWap .slide_list{
	font-weight: 600;
	border-bottom: 1px solid #e5e5e5;
	color: #666;
	font-size: 14px;
	height: 43px;
	line-height: 43px;
}
.hotel_list .list  .slideWap .slide_list:hover{
	background-color: #fff;
}
.hotel_list .list .slideWap .otherInfo img {
	width: 130px;
	height: 100px;
	margin-right: 8px;
	margin-top: 15px;
}
.hotel_list .list .slideWap .otherInfo .msg{
	padding: 14px 0;
	font-size: 14px;
	color: #999;
}

.hotelInfo_box {
	border-radius: 4px;
	background-color: #fff;
	width: 1200px;
	/*height: 600px;*/
	margin: 18px auto 0;
}
.hotelInfo_box .title,
.trafficInfo .title,
.userComment .title{
	border-bottom: 1px solid #e5e5e5;
	height: 57px;
	line-height: 57px;
	font-size: 18px;
	color:#659ff5;
	font-family: PingFang-SC-Bold;
}
.hotelInfo_box .title span ,
.trafficInfo .title span ,
.userComment .title span {
	display: inline-block;
	height: 54px;
	margin-left: 29px;
	border-bottom: 3px solid #659ff5;
}
.hotelInfo_box .infoWap{
	padding:0 32px;
} 
.hotelInfo_box .infoWap .tit{
	font-family: PingFang-SC-Bold;
	height: 52px;
	line-height: 52px;
} 
.hotelInfo_box .infoWap .info {
	color: #999;
	border-bottom: 1px solid #e5e5e5;
	padding-bottom: 18px;
}
.hotelInfo_box .list{
	position: relative;
	color: #999;
	padding: 15px 0 15px 135px;
	border-bottom: 1px solid #e5e5e5;
} 
.hotelInfo_box .list .tit {
	position: absolute;
	top: 0;
	left: 0;
	color: #333;
	font-family: PingFang-SC-Bold;
}

.trafficInfo{
	margin: 8px auto 0;
	width: 1200px;
	height: 438px;
	background-color: #ffffff;
	box-shadow: 0px 0px 10px 0px 
		rgba(0, 0, 0, 0.04);
	border-radius: 4px;
}
.bigmap {
	padding: 25px 32px;
	width: 1136px;
	height: 326px;
	background-color: #ffffff;
	border-radius: 4px;
}
#bigmap{
	width: 100%;
	height: 100%;
} 
.userComment{
	margin: 8px auto 96px;
	width: 1200px;
	background-color: #ffffff;
	box-shadow: 0px 0px 10px 0px 
		rgba(0, 0, 0, 0.04);
	border-radius: 4px;
}
.userComment .count {
	position: relative;
	margin: 0 auto;
	width: 1027px;
	padding:24px 0 24px 115px;
}
.userComment .count .con_1{
	font-size: 18px;
	color:#999;
}
.userComment .count .con_1 span  {
	margin-right: 28px;
}
.userComment .count .con_1 em {
	margin-left: 5px;
	font-weight: 600;
	font-style: normal;
	color: #659ff5;
}
.userComment .count .tag  {
	margin-top: 12px;
}
.userComment .count .tag span{
	margin-right: 8px;	
	padding: 0 8px;
	line-height: 24px;
	color: #666;
	font-size: 14px;
	background-color: #f5f5f5;
	border-radius: 4px;
}
.userComment .count .tot_count{
	text-align: center;
	position: absolute;
	top: 18px;
	left: 0;
	width: 98px;	
	height: 65px;
	border-right: 1px solid #e5e5e5;
	font-size: 18px;
	color: #659ff5;
	font-family: PingFang-SC-Bold;
}

.userComment .count .tot_count .c {
	font-size: 36px;
	line-height: 36px;
}

.cstatistics {
	margin: 0 auto;
	width: 1142px;
	height: 42px;
	line-height: 42px;
	background-color: #fafafa;
	border-radius: 2px;
}
.cstatistics span {
	padding: 0 25px;
	border-right: 1px solid #e5e5e5;
	color: #999;
}
.cstatistics span:last-child{
	border:none;
}
.cstatistics span.active ,
.cstatistics span:hover {
	color: #333;
}
.userComment .comment_list{
	margin: 0 auto;
	width: 1142px;
	height: 600px;
}
.userComment .comment_list .list {
	position: relative;
	padding: 15px 10px 15px 150px; 
	width: 983px;
	font-family: PingFang-SC-Medium;
	font-size: 14px;
	color: #333333;
	border-bottom: 1px solid #e5e5e5;
}

.userComment .comment_list .list .type{
	color: #999;
	margin-bottom: 6px;
}
.userComment .comment_list .list .type .hotel {
	border-right: 1px solid #e5e5e5;
	padding-right: 5px;
}
.userComment .comment_list .list .type .star i{
	margin-right: 3px;
	color: #f5a21b;
	font-style: normal;
} 
.userComment .comment_list .list .time {
	color: #999;
	margin-top: 5px;
}
.userComment .comment_list .list .comment_user{
	position: absolute;
	top: 18px;
	left: 20px;
	text-align: center;
}

.userComment .comment_list .list .comment_user .headImg {
	width: 46px;
	height: 46px;
	border-radius: 50%;
	overflow: hidden;
	margin: 0 auto;
	margin-bottom: 8px;
}
.tooltip{
	position: fixed;
	
}
.main {
	width: 1200px;
	margin: 0 auto;	
	padding-top: 68px;
	margin-bottom: 80px;
}
.hide{
	display: none;
}
.bread {
	padding: 20px 0 20px 15px;
	font-family: PingFang-SC-Medium;
	font-size: 14px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #333333;
	position:relative;          
}   
.bread .listSearch {
	position: absolute;
	top: 16px;
	right: 0;
}
.bread  .searchNum {
	width: 174px;
	height: 32px;
	background-color: #ffffff;
	border-radius: 4px;
	border: solid 1px #e5e5e5;
	padding-left: 14px;
	margin-left: 17px;
}   
.rightList {
	width: 946px;
	/*height: 500px;*/
	/*background-color: pink;*/
} 
.limit_wap{
	width: 902px;
	height: 64px;
	background-color: #fbfcfd;
	border-radius: 4px 4px 0px 0px;
	padding:0 22px;
	border-bottom: 1px solid #e5e5e5;
	position: relative;
}

#listType{
	width: 106px;
	height: 32px;
	background-color: #ffffff;
	border-radius: 4px;
	border: solid 1px #e5e5e5;
	padding-left: 14px;
	margin-top: 16px;
}    
.limit_wap  .status {
	color:#999;
	display: inline-block;
}  
.limit_wap  .status .active {
	color:#333;
	font-size: 16px;
	font-family: PingFang-SC-Bold;
}
.limit_wap  .status > span {
	margin-left: 35px;
	cursor: pointer;
} 
/* .limit_wap .searchdate {
	position: absolute;
	top: 16px;
	right: 0;
} */
.limit_wap .searchdate .date_box {
	margin-top: 16px;
	display: inline-block;
	/*width: 284px;*/
	height: 32px;
	background-color: #ffffff;
	border-radius: 4px 0 0 4px;
	border: solid 1px #e5e5e5;
	border-right: none;
}
.limit_wap .searchdate  input {
	border:none;
	height: 100%;
}
.searchdate .date_box .satrtDate {
	border-right: 1px solid #e5e5e5;
	border-radius: 4px 0 0 4px;
}
 .limit_wap .searchdate .search{
 	display: inline-block;
	width: 36px;
	height: 32px;
	background-color: #659ff5;
	border-radius: 0px 4px 4px 0px;
	vertical-align: middle;
	margin-left: -4px;
 }
.allist {
	color: #999;
	font-family: PingFang-SC-Medium;
}
.color_3{
	color: #333;
}
.allist .list_tit {
	height: 40px;
	background-color: #fafafa;
	padding: 0 30px; 
	color:#999;	
	line-height: 40px;
}
.allist .list_tit .delist {
	font-size: 14px;
	color: #333;
}
.allist .list_tit .delist:hover {
	text-decoration:underline;
}
.allist .list_tit .color_3{
	margin-right: 55px;
}
.rightList .tab_title {
	background-color: #fff;
	color: #999;
	font-family: PingFang-SC-Bold;
	height: 46px;
	line-height: 46px;
	padding:0 30px;
}

.rightList .tab_title span {
	display: inline-block;
	vertical-align: middle;
}
.len1{
	width: 250px;
} 
.len2{
	width: 78px;
} 
.len3{
	width: 120px;
} 
.len4{
	width: 100px;
} 
.len5{
	width: 115px;
} 
.len6{
	width: 110px;
}  
.len7{
	width: 80px;
}
.list_info {
	background-color: #fff;
	padding: 28px 30px;
	font-size: 14px;
}
.list_info .left_img{
	width: 65px;
	height: 65px;
	margin-right: 12px;
} 
.list_info  .list_name {
	font-family: PingFang-SC-Bold;
	font-size: 16px;
	color: #333333;
	text-overflow:ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow:hidden;
}
.list_info  .right_info {
	width: 170px;
}
.len {
	display: inline-block;
	vertical-align: middle;
}
.topay{
	display: inline-block;
	width: 76px;
	height: 28px;
	background-color: #659ff5;
	border-radius: 4px;
	text-align: center;
	line-height:  28px;
	color: #fff;
	cursor: pointer;
}
.topay:hover {
	background-color: rgba(116,173,255);
}
.tocancel{
	display: inline-block;
	width: 76px;
	height: 28px;
	background-color: #fff;
	border-radius: 4px;
	text-align: center;
	line-height:  28px;
	cursor: pointer;
}
.tocancel:hover {
	text-decoration:underline;
}
.toreturn {
	display: inline-block;
	width: 76px;
	height: 28px;
	background-color: #fff;
	border-radius: 4px;
	text-align: center;
	line-height:  28px;
	cursor: pointer;
	border: 1px solid #659ff5;
	color:#659ff5;
	vertical-align: middle;
}
.len .hide {
	display: none;
}
.toreturn:hover {
	text-decoration:underline;
}
.orderdetail {
	color: #659ff5;
}
.orderdetail:hover{
	color: rgba(116,173,255);
}
.order_list {
	position: relative;
	transition: all 0.2s;
}
.order_list:hover {
	box-shadow: 0px 1px 6px 0px 
		rgba(0, 0, 0, 0.08);
		z-index: 10;
}
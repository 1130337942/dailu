body{
	background-color: #525659;
	height: auto;
}
.main {
	position: relative;
	background-color: #fff;
	width: 720px;
	padding: 50px 35px;
	margin: 0 auto;
	padding-bottom: 300px;
}
.main img {
	width: 100%;
	height: auto;
}
.main .sur {
	min-height: 1075px;
	border-bottom: 1px solid #ccc;
	/*padding-bottom: 50px;*/
}
.main .sur .line_cros{
	color: #666;
	text-align: center;
}
.main .sur .line_tit{
	text-align: center; 
	font-size: 20px;
	font-weight: 600;
	line-height: 70px;
}
.main .sur .line_time {
	text-align: center;
	font-size: 14px;
	color: #999;
	line-height: 50px;
	margin-bottom: 50px;
}
.main .sur .ewm_box{
	width: 120px;
	height: 120px;
}
.main .sur .user {
	padding-top: 100px;
}
.main .sur .ewm_box img {
	width: 100%;
	height: 100%;
}
.summarize {
	min-height: 1125px;
}
.summarize .sum_title {
	margin-top: 20px;
	position: relative;
	font-size: 30px;
	color:#4a99ff;
	font-weight: 600;
	margin-bottom: 50px;
}
.summarize .sum_title::before {
	width: 18px;
	height: 78px;
	content: '';
	background-color: #00beff;
	position: absolute;
	top: 0;
	left: -35px;
}
.summarize .sum_title::after {
	position: absolute;
	bottom: 0;
	right: 0;
	content: '';
	width: 480px;
	height: 1px;
	background-color: #00beff;
}
.summarize ul {
	line-height: 80px;
}
.travel_line  {
	min-height: 1125px;
}
.travel_line .travel_title {
	color: #666;
	font-size: 30px;
	text-indent:7em;
	position: relative;
	line-height: 80px;
}
.travel_line .travel_title::after {
	content: '行程线路';
	display: block;
	width: 288px;
	height: 80px;
	line-height: 80px;
	text-align: center;
	color: #fff;
	background-color: #00beff;
	position: absolute;
	top: 0;
	right: -31px;
	text-indent: 0em;
}

.travel_line .line_img {
	margin-top: 20px;
	width: 100%;
	height: 480px;
}
.travel_line .line_img img {
	width: 100%;
	height: 100%;
}
.city_catalog{
	font-size: 18px;
    line-height: 52px;
    position: relative
}

.travel_line .city_catalog:first-child .day,
.travel_line .city_catalog:last-child .day{
	color:#999;
}

.city_catalog .zxq_ico {
	font-style: normal;
    display: inline-block;
    width: 22px;
    height: 22px;
    background-color: #659ff5;
    border-radius: 50%;
    color: #fff;
    text-align: center;
    line-height: 22px;
    font-size: 14px;
    position: relative;
    margin-right: 10px;
}

.travel_line .city_catalog .day{
	font-size: 18px;
}

.city_catalog .zxq_ico::before {
    display: block;
    content: '';
    width: 2px;
    height: 20px;
    background-color: #e5e5e5;
    position: absolute;
    left: 9px;
    top: 24px;
}
.travel_line .city_catalog:first-child .zxq_ico::after,
.travel_line .city_catalog:last-child .zxq_ico::after {
    display: block;
    content: '';
    width: 16px;
    height: 16px;
    border-radius: 50%;
    background-color: #fff;
    position: absolute;
    top: 3px;
    left: 3px;
}

 .city_catalog::before {
    display: block;
    content: '';
    width: 400px;
    height: 0;
    border-bottom: 1px dashed #ccc;
    position: absolute;
    top: 50%;
    left: 140px;
}
.travel_line .city_catalog:last-child .zxq_ico::before {
	display: none;
}

.travel_line .city_catalog:last-child .zxq_ico {
    background-color: #f4a21e;
}
.clost {
	min-height: 1125px;
}
.clost .title {
	position: relative;
	margin-bottom: 50px;
}
.clost .title .t {
	font-size: 30px;
	color: #0099FF;
}
.clost .title::before{
	width: 18px;
    height: 78px;
    content: '';
    background-color: #00beff;
    position: absolute;
    top: 0;
    left: -35px;
}
.clost .title::after{
	width: 230px;
    height: 1px;
    content: '';
    background-color: #00beff;
    position: absolute;
    top: 50%;
    left: 187px;
}
.clost .title .num {
	margin-right: 60px;
	font-size: 16px;
	color: #666;
	line-height: 40px;
}
.clost .title .num em{
	color:#FF0000;
	font-style:normal;
	font-size: 18px;
	font-weight: 600;
}
.clost .cost_tab .cost_list {
	color: #999;
}
.clost .cost_tab .cost_list em {
	font-style:normal;
	color: #FF6666;
}
.clost .cost_tab .cost_list .tab_head{
	width: 300px;
	height: 44px;
	display: inline-block;
	overflow: hidden;
	color: #666;
}
.clost .cost_tab .cost_list .tab_head i {
	display: inline-block;
	width: 28px;
	height: 28px;
	vertical-align: middle;
	margin-top: -5px;
	margin-right: 15px;
}
.clost .cost_tab .cost_list .tab_head i.plane {
	background: url(/static/v1/img/tripinfoShare/plane1.png) center center no-repeat;
	background-size: 80% 85%;
}
.clost .cost_tab .cost_list .tab_head i.train {
	background: url(/static/v1/img/tripinfoShare/train1.png) center center no-repeat;
	background-size: 80% 85%;
}
.clost .cost_tab .cost_list .tab_head i.hotel {
	background: url(/static/v1/img/tripinfoShare/hotel1.png) center center no-repeat;
	background-size: 80% 85%;
}
.clost .cost_tab .cost_list .tab_head i.canyin {
	background: url(/static/v1/img/tripinfoShare/canyin.png) center center no-repeat;
	background-size: 80% 85%;
}
.clost .cost_tab .cost_list .tab_price{
	width: 140px;
	height: 44px;
	display: inline-block;
	overflow: hidden;
}
.clost .cost_tab .cost_list  {
	margin-top: 15px;
}
.clost .cost_tab .cost_list .tab_num{
	width: 110px;
	height: 44px;
	overflow: hidden;
	display: inline-block;
	text-align: right;
}

.clost .cost_tab .cost_list .tab_tot{
	width: 140px;
	height: 44px;
	overflow: hidden;
	display: inline-block;
	text-align: right;
}
.clost .cost_tab .cost_list .cost_list_item{
	overflow: hidden;
	height: 44px;
	line-height: 44px;
}
.clost .cost_tab .cost_list .cost_list_tit {
	height: 44px;
	line-height: 44px;
	background-color: rgba(242, 242, 242, 1);
}
.eahart{
	margin-top: 30px;
	min-height: 300px;

}
/* 日程安排 */
.day_wap .title {
	position: relative;
	margin-bottom: 50px;
	font-size: 30px;
	color: #0099FF;
}
.day_wap .title::before{
	width: 18px;
    height: 78px;
    content: '';
    background-color: #00beff;
    position: absolute;
    top: 0;
    left: -35px;
}
.day_wap .title::after{
	width: 500px;
    height: 1px;
    content: '';
    background-color: #00beff;
    position: absolute;
    bottom: 0;
    right: 0;
}
.day_wap .daylist{
	margin-bottom: 20px;
	border-bottom: 2px solid #ccc;
}
.day_wap .daylist:last-child{
	border-bottom: none;
}
.day_wap .daylist .tit{
	font-size: 20px;
	color: #333;
	font-weight: 600;
}
.day_wap .daylist .tit .day_index {
	color: #00beff;
	margin-right: 20px;
}
.day_wap .daylist .tit .use_time {
	margin-left: 70px;
	font-size: 16px;
	color: #999;
}
.day_wap .daylist .tit  .date {
	font-size: 18px;
	color: #666;
}
.day_wap .daylist .city_map {
	width: 100%;
	height: 480px;
	margin: 30px 0;
}
.day_wap .daylist .city_map img {
	width: 100%;
	height: 100%;
}

.day_wap .inner_box {
    padding-left: 20px;
    border-left: 2px solid #659ff5;
}
.day_wap  .inner_box .circle,
.day_wap  .inner_box .point {
    position: relative;
}
.inner_box .inner_tit {
	font-size: 20px;
	font-weight: 600;
	position: relative;
}
.inner_tit .cityName{
	color: #333;
}
.inner_tit .tip{
	color: #666;
}
.day_wap .inner_box .p_line {
    color: #659ff5;
}


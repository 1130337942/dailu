.inner_box .city2city{
	padding-left: 12px;
	background-color: #dedcdc;
	font-size: 14px;
	line-height: 36px;
	margin: 10px 0;
}

.day_wap .inner_box .circle::before {
    position: absolute;
    display: block;
    background-color: #fff;
    content: '';
    width: 10px;
    height: 10px;
    border: 4px solid #659ff5;
    border-radius: 50%;
    top: 4px;
    left: -30px;
}
.day_wap .inner_box .point::before{
	position: absolute;
	display:block;
	content: '';
	width: 10px;
	height: 10px;
	background-color: #659ff5;
	border-radius: 50%;
	top: 12px;
	left: -26px;
}
.inner_box .inner_msg {
	font-size: 16px;
	color: #666;
}

.day_wap .inner_box .wap_ico{
	display: inline-block;
	width: 20px;
	height: 20px;
	vertical-align: middle;
	margin-right: 8px;
}
.day_wap .inner_box .wap_ico.train {
	background:  url(/static/v1/img/addStrokeform/train.png) center center no-repeat;
} 
.day_wap .inner_box .wap_ico.plane {
	background:  url(/static/v1/img/addStrokeform/air.png) center center no-repeat;
}
.day_wap .inner_box .wap_ico.bus {
	background:  url(/static/v1/img/addStrokeform/bus.png) center center no-repeat;
}
.day_wap .inner_box .wap_ico.hotel {
	background:  url(/static/v1/img/map/hotel.png) center center no-repeat;
}
.day_wap .inner_box .wap_ico.jd {
	background:  url(/static/v1/img/map/jd.png) center center no-repeat;
}
.day_wap .inner_box .wap_ico.cy {
	background:  url(/static/v1/img/map/t_food.png) center center no-repeat;
}

.day_wap .inner_box .wap_ico.taxi {
	background:  url(/static/v1/img/map/taxi.png) center center no-repeat;
}
.day_wap .inner_box .hotel_wap{
	margin-bottom: 35px;
	font-size: 14px;
}
.day_wap .inner_box  .deeptime {
	background-color: #dedcdc;
	line-height: 36px;
	padding-left: 12px;
	color:#666;
	font-size: 14px;
	margin: 10px 0;
}
.day_wap .inner_tit em {
    font-style: normal;
    color: #fff;
    font-size: 12px;
    background-color: #659ff5;
    display: block;
    width: 16px;
    height: 16px;
    border-radius: 50%;
    line-height: 16px;
    text-align: center;
    position: absolute;
    top: 5px;
    left: -29px;
}
.day_wap .inner_box .spot_info{
	margin: 20px 0;
	font-size: 16px;
	color: #666;
}
.day_wap .inner_box .spot_info .img_box{
	width: 300px;
	height: 200px;
}
.day_wap .inner_box .spot_info .spot_msg {
    width: 370px;
    /*height: 124px;*/
    padding-top: 10px;
    overflow: hidden;
}

#p-left {
	height: 300px;
}

.eahart li {
	margin-right: 30px;
}
.eahart li span {
    vertical-align: middle;
    display: inline-block;
    width: 8px;
    height: 8px;
    background-color: #028bff;
    border-radius: 4px;
    margin-right: 4px;
}
.download_pdf {
	position: absolute;
    top: 100px;
    right: -100px;
    padding: 10px;
    background-color: #659ff5;
    color: #fff;
    cursor: pointer;
}
.download_pdf:hover {
	background-color: #4a8ae9;
}
.mask {
	z-index: 12;
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background-color: rgba(255,255,255,0);

}
.mask .tip_content {
	margin:300px auto;
	text-align: center;
	width: 200px;
	height: 160px;
	border-radius: 8px;
	background-color: #fff;
	box-shadow: 0px 4px 12px 0px rgba(0, 0, 0, 0.12);
	/*margin-top: 40%;	*/
}
.mask .tip_content img {
	width: 100px;
	height: 100px;
}
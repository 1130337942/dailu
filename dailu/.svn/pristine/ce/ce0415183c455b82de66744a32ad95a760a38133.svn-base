/* 手风琴 */

@-webkit-keyframes fadeInDownx {
    0% {
        
        -webkit-transform: translate3d(0, -100%, 0) skewX(-5deg);
        transform: translate3d(0, -100%, 0) skewX(-5deg);
    }
    100% {
       
        -webkit-transform: skewX(-5deg);;
        transform: skewX(-5deg);
    }
}

@keyframes fadeInDownx {
    0% {
       
        -webkit-transform: translate3d(0, -100%, 0) skewX(-5deg);
        -ms-transform: translate3d(0, -100%, 0) skewX(-5deg);
        transform: translate3d(0, -100%, 0) skewX(-5deg);
    }
    100% {
        
        -webkit-transform: skewX(-5deg);
        -ms-transform: skewX(-5deg);
        transform: skewX(-5deg);
    }
}


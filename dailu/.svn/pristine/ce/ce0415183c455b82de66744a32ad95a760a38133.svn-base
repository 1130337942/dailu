        transform: translate3d(0, 20%, 0)
    }
    100% {
        opacity: 1;
        -webkit-transform: none;
        transform: none
    }
}

@keyframes fadeInUpx {
    0% {
        opacity: 0;
        -webkit-transform: translate3d(0, 20%, 0);
        -ms-transform: translate3d(0, 20%, 0);
        transform: translate3d(0, 20%, 0)
    }
    100% {
        opacity: 1;
        -webkit-transform: none;
        -ms-transform: none;
        transform: none
    }
}

.fadeInUpx {

/* 改变垂直轮播分页器 */
.swiper-pagination-v{
    opacity: 1;
    transition: all 0.8s ease;
    -webkit-transition: all 0.8s ease;
    -o-transition: all 0.8s ease;
    -ms-transition: all 0.8s ease;
    -moz-transition: all 0.8s ease;
}
.swiper-pagination-v::before{
    content: "|";
    position: absolute;
    top: -20px;
    left: 8px;
    color:rgba(253, 250, 250,0.9);
}
.swiper-pagination-v::after{
    content: "|";
    position: absolute;
    bottom: -20px;
    left: 8px;
    color:rgba(253, 250, 250,0.9);
}
.swiper-pagination-v .swiper-pagination-bullet{
    width: 20px;
    height: 24px;
    line-height: 24px;
	font-size: 16px;
    background-color: rgba(0,0, 0, 0);
    color:rgb(253, 250, 250);
    opacity: 0.8;

}
.swiper-container-vertical>.swiper-pagination-v{
    right: 4%;
    width: 20px;
    font-family: dl;
    
}
.swiper-pagination-v .swiper-pagination-bullet-active{
    position: relative;
    right: 28px;
    font-size: 18px;
    opacity: 1;

    transition: all 0.8s ease;
    -webkit-transition: all 0.8s ease;
    -o-transition: all 0.8s ease;
    -ms-transition: all 0.8s ease;
    -moz-transition: all 0.8s ease;
}
.swiper-pagination-bullet-active::before{
    content: "";
    width: 15px;
    height: 3px;
    background-color: #ffffff;
    position: absolute;
    font-size: 10px;
    left: 30px;
    top: 10px;

}



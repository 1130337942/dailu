.banner_line{
    position: absolute;
    left: 0px;
    top: 0px;
    z-index: 1;
    vertical-align: middle;
    height: 100%;
    width: 100%;
    overflow: hidden;
}
.banner_span,.banner_text,.hot_mall_box{
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0px;
    top: 0px;
}
.banner_span span{
    position: absolute;
    width: 10px;
    height: 10px;
    background: #fff;
    border-radius: 50%;
    z-index: 2;
    color: white;
    opacity: 0;

    transition: all 1s ease;
    -webkit-transition: all 1s ease;
    -o-transition: all 1s ease;
    -ms-transition: all 1s ease;
    -moz-transition: all 1s ease;
}
.banner_span span::before{
    content: "";
    position: absolute;
    z-index: 1;
    width: 10px;
    height: 10px;
    border-width: 12px;
    border: solid 12px  rgba(255, 255, 255, 0.3);;
    border-radius: 50%;
    left: calc(5px - 17px);
    top: calc(5px - 17px);
}
.banner_span span::after{
    font-size: 0px;
    position: absolute;
    z-index: 2;
    width: 60px;
    height: 60px;
    /* left: calc(5px - 30px); */
    top: calc(5px - 30px);
    margin-top: 160px;
    margin-left: -10px;
    opacity: 0;
    font-family: dl;

    background: rgb(224,150,0);
    background: -moz-linear-gradient(left, rgba(224,150,0,1) 0%, rgba(253,251,0,1) 45%);
    background: -webkit-linear-gradient(left, rgba(224,150,0,1) 0%,rgba(253,251,0,1) 45%);
    background: linear-gradient(to right, rgba(224,150,0,1) 0%,rgba(253,251,0,1) 45%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#e09600', endColorstr='#fdfb00',GradientType=1 );
    background-clip: text ;
    -webkit-background-clip: text ;
    -webkit-text-fill-color: transparent ;

    /* transition: all 1.5s ease-out;
    -webkit-transition: all 1.5s ease-out;
    -o-transition: all 1.5s ease-out;
    -ms-transition: all 1.5s ease-out;
    -moz-transition: all 1.5s ease-out; */

    transition: all 1s ease;
    -webkit-transition: all 1s ease;
    -o-transition: all 1s ease;
    -ms-transition: all 1s ease;
    -moz-transition: all 1s ease;
}


}
.mainWap .formWap .tab_bar span{
	text-align: center;
	font-size: 14px;
	color:#999;
	display: inline-block;
	line-height: 30px;
	width: 88px;
	height: 30px;
	border-radius: 4px;
	border: solid 1px #e5e5e5;
	cursor: pointer;
}
.mainWap .formWap .tab_bar{
	height: 52px;
}
.mainWap .formWap .tab_bar span:hover,
.mainWap .formWap .tab_bar span.active {

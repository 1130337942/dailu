.mainWap .formWap  .c_type {
	position: relative;
	padding-left: 15px;
	width: 98px;
	height: 30px;
	border-radius: 4px;
	border: solid 1px #e5e5e5;
}
.mainWap .formWap  .trig {
	vertical-align: middle;
	margin-left: -20px;
	margin-top: -5px;
	position: relative;
	z-index: 2;
	display: inline-block;
	content:'';
	width: 0;
	height: 0;
	border-bottom:3px solid #999;
	border-right:3px solid #999;
	border-top: 3px solid transparent;
	border-left: 3px solid transparent;
	transform: rotate(45deg);
}
.mainWap .formWap .list .w_info{
	width: 336px;
	padding-left: 12px;
	height: 30px;
	border-radius: 4px;

}
.mainWap .formWap .list .phone_number {
	width: 192px;
	padding-left: 12px;
	height: 30px;
	border-radius: 4px;
	border: solid 1px #e5e5e5;
}
.mainWap .formWap .list .email{
	width: 336px;

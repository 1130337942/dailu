<body style='background-color:#f6f6f6'>
	<include file="public@public_header_store"/>
		<div class="minWap clearfix">
			<div class="leftWap fl">
				<div class="trainInfo">
					<div class="title">车次信息展示 <i></i> 
						<span class="tip">发车日期：5月24日</span><a class="tip fr" href="javascript:;">退改签说明</a>
					</div>
					<div class="train clearfix">
						<div class="fl wap1">
							<div class="fz24">D3126</div>
							<div class="fz14">修改车次</div>
						</div>
						<div class="fl wap2">
							<div class="leave inblock">
								<div class="fz24">06:43</div>	
								<div class="fz14">上海虹桥</div>								
							</div>
							<div class="arr inblock">
								5时57分
							</div>
							<div class="deast inblock">
								<div class="fz24">09:43</div>	
								<div class="fz14">南京北</div>
							</div>
						</div>
						<div class="fr wap3">
							<div class="fz24 money">¥ 568</div>
							<div class="type fz14">二等座</div>
						</div>
					</div>
				</div>
				<div class="passenger">
